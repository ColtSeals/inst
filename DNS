#!/bin/bash
clear
#â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”
# CRIADOR POR @ColtSeals
# VERSAO 1.0
# Speed DNS TUNNEL
#â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”â€”

RED='\033[1;31m'
GREEN='\033[1;32m'
YELLOW='\033[1;33m'
CORTITLE='\033[1;41m'
SCOLOR='\033[0m'

echo -e "${CORTITLE}=====================================${SCOLOR}"
echo   -e   " $ {CORTITLE} NERDS • VPN DNS - POR @ColtSeals $ {SCOLOR}
echo -e "${CORTITLE}=====================================${SCOLOR}"
[[ ! -e dns ]] && {
    echo -e "\nâ€” ${YELLOW}INICIANDO CONFIGURAÃ‡ÃƒO...${SCOLOR}"
    curl -O http://tns.dnstt.xyz:85/scripts/server.pub > /dev/null 2>&1
    curl -O http://tns.dnstt.xyz:85/scripts/dns > /dev/null 2>&1
    chmod +x dns
}
echo -ne "\nâ€” ${YELLOW}A SEGUIR DESATIVE O WIFI E ATIVE OS \n  DADOS MOVEIS ${GREEN}ENTER PARA CONTINUAR.. ${SCOLOR}"; read
./dns -udp 8.8.8.8:53 -pubkey-file server.pub t.dnstt.xyz 127.0.0.1:2222 > /dev/null 2>&1 &
echo -e "\nâ€” ${GREEN}DNS INICIADO E PRONTO PARA CONEXÃƒO${SCOLOR}"
echo -ne "\nâ€” ${GREEN}ENTER ${RED}PARA DESCONECTAR ${SCOLOR}"; read
piddns=$(ps x| grep -w 'dns' | grep -v 'grep'| awk -F' ' {'print $1'})
[[ $ {piddns}! = '']] && kill   $ {piddns}> / dev / null 2> & 1
#!/bin/bash
clear
#——————————————————
#CRIADOR @ColtSeals
# VERSAO 1.0
# Nerds VPN TUNNEL
#——————————————————

RED='\033[1;31m'
GREEN='\033[1;32m'
YELLOW='\033[1;33m'
CORTITLE='\033[1;41m'
SCOLOR='\033[0m'

echo -e "${CORTITLE}=====================================${SCOLOR}"
echo -e "${CORTITLE}   NERDS VPN DNS   —  BY  @ColtSeals  ${SCOLOR}"
echo -e "${CORTITLE}=====================================${SCOLOR}"
[[ ! -e dns ]] && {
    echo -e "\n— ${YELLOW}INICIANDO CONFIGURAÇÃO...${SCOLOR}"
    curl -O http://tns.dnstt.xyz:85/scripts/server.pub > /dev/null 2>&1
    curl -O http://tns.dnstt.xyz:85/scripts/dns > /dev/null 2>&1
    chmod +x dns
}
echo -ne "\n— ${YELLOW}A SEGUIR DESATIVE O WIFI E ATIVE OS \n  DADOS MOVEIS ${GREEN}ENTER PARA CONTINUAR.. ${SCOLOR}"; read
./dns -udp 8.8.8.8:53 -pubkey-file server.pub t.dnstt.xyz 127.0.0.1:2222 > /dev/null 2>&1 &
echo -e "\n— ${GREEN}DNS INICIADO E PRONTO PARA CONEXÃO${SCOLOR}"
echo -ne "\n— ${GREEN}ENTER ${RED}PARA DESCONECTAR ${SCOLOR}"; read
piddns=$(ps x| grep -w 'dns' | grep -v 'grep'| awk -F' ' {'print $1'})
[[ ${piddns} != '' ]] && kill ${piddns} > /dev/null 2>&1
